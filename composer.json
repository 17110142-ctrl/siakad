{
    "name": "siakad/smpmuhammadiyahsuruh",
    "description": "Aplikasi SIAKAD SMP Muhammadiyah Suruh dengan PhpSpreadsheet",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "phpoffice/phpspreadsheet": "^1.18"
    },
    "config": {
        "platform": {
            "php": "7.3.33"
        },
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
